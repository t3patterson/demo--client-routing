{
  "name": "webpack-simple-server",
  "version": "1.0.0",
  "description": "starter kit for webpack-es6",
  "main": "app.js",
  "scripts": {
    "start": "node --use_strict server.js",
    "dev": "npm-run-all --parallel dev-server webpack-watcher",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:production": "webpack",
    "dev-server": "NODE_ENV=development node server.js",
    "webpack-watcher": "webpack --progress --watch --config webpack.config.dev.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "director": "1.2.8",
    "ejs": "2.5.6",
    "express": "4.14.1",
    "superagent": "3.6.0"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "0.10.1",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.1",
    "resolve-url-loader": "^2.0.0",
    "sass-loader": "5.0.1",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.1",
    "webpack-middleware": "1.5.1"
  }
}
